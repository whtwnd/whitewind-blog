    "flowbite": "^2.5.2",
    "flowbite-react": "^0.10.2",
    "github-markdown-css": "^5.5.1",
    "hast-util-is-javascript": "^3.0.1",
    "highlight.js": "^11.10.0",
    "html-react-parser": "^5.2.3",
    "jotai": "^2.10.3",
    "multiformats": "^13.3.1",
