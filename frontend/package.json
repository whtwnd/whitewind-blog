{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "rm -rf .next/cache/fetch-cache && next dev --turbopack",
    "build": "next build",
    "start": "cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/ && HOSTNAME=localhost && node .next/standalone/server.js",
    "lint": "ts-standard",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "tailwind_bsky": "npx tailwindcss -i ./src/components/bskyembed/src/index.css -o ./src/components/bskyembed.css --watch"
  },
  "ts-standard": {
    "project": "/workspaces/frontend/tsconfig.json",
    "ignore": [
      "src/api/**/*.js",
      "src/api/**/*.ts",
      "src/api/**/*.jsx",
      "src/api/**/*.tsx",
      "next-env.d.ts",
      "src/components/bskyembed/*"
    ]
  },
  "dependencies": {
